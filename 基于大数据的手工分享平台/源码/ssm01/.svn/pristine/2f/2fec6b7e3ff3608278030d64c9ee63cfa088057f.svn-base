package ssm.pojo.Goods;

public class GoodsCommentImageEntity {

	private String img_path;
	private int img_order;

	public GoodsCommentImageEntity() {
		super();
		// TODO Auto-generated constructor stub
	}

	public GoodsCommentImageEntity(String img_path, int img_order) {
		super();
		this.img_path = img_path;
		this.img_order = img_order;
	}

	public String getImg_path() {
		return img_path;
	}

	public void setImg_path(String img_path) {
		this.img_path = img_path;
	}

	public int getImg_order() {
		return img_order;
	}

	public void setImg_order(int img_order) {
		this.img_order = img_order;
	}

	@Override
	public String toString() {
		return "GoodsCommentImageEntity [img_path=" + img_path + ", img_order=" + img_order + "]";
	}
}
